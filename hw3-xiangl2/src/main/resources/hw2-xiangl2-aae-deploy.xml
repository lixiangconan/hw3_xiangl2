  xmlns="http://uima.apache.org/resourceSpecifier">
  
  <name>hw2-xiangl2-aae-deploy</name>
  <description>Deploy hw2-xiangl2-aae in local machine. </description>
  
  <deployment protocol="jms" provider="activemq">
    <service>
      <inputQueue endpoint="hw2-xiangl2-aae-queen" brokerURL="${defaultBrokerURL}"/>
